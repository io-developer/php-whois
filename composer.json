{
    "name": "io-developer/php-whois",
    "description": "PHP WHOIS provides parsed and raw whois lookup of domains and ASN routes. PHP 8.1+ compatible (down to 5.4 in prev versions)",
    "keywords": [
        "php",
        "whois",
        "query",
        "tld",
        "domain",
        "lookup",
        "info",
        "asn",
        "routes",
        "parser",
        "црщшы"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/io-developer/php-whois",
    "authors": [
        {
            "name": "Sergey Sedyshev",
            "email": "i.o.developer@gmail.com",
            "homepage": "https://github.com/io-developer"
        }
    ],
    "require" : {
        "php" : ">=8.1",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "symfony/polyfill-intl-idn": "*",
        "psr/container": "*"
    },
    "require-dev" : {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Iodev\\": "src/Iodev/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": [ "tests/" ]
        }
    },
    "scripts": {
        "test": [
            "phpunit --bootstrap tests/bootstrap.php tests"
        ]
    }
}
