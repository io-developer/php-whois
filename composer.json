{
    "name": "io-developer/php-whois",
    "description": "PHP WHOIS provides parsed and raw-text whois lookup info of domains. PHP 5.4+ and 7+ compatible",
    "keywords": [
        "php",
        "whois",
        "client",
        "pure",
        "domain",
        "information",
        "availability",
        "registrant",
        "registrar",
        "expiry",
        "expiration",
        "parser",
        "lookup",
        "idna",
        "punycode",
        "unicode",
        "црщшы"
    ],
    "type": "library",
    "license": "MIT",
    "version": "2.3.0",
    "homepage": "https://github.com/io-developer/php-whois",
    "authors": [
        {
            "name": "Sergey Sedyshev",
            "email": "i.o.developer@gmail.com",
            "homepage": "https://github.com/io-developer"
        }
    ],
    "require" : {
        "php" : ">=5.4",
        "ext-intl": "*",
        "ext-mbstring": "*"
    },
    "require-dev" : {
        "phpunit/phpunit": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Iodev\\": "src/Iodev/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": [ "tests/" ]
        }
    },
    "scripts": {
        "test": [
            "phpunit --bootstrap tests/bootstrap.php tests"
        ]
    }
}
